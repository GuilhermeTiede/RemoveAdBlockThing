{
    "manifest_version": 2,
    "name": "Remove Adblock Thing Extension",
    "version": "1.0",
    "description": "Remove Adblock Thing on YouTube",
    "permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "icons": {
      "48": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "Remove Adblock Thing Extension"
    }
  }
  